{
	"name": "cline-chinese",
	"displayName": "Cline Chinese",
	"description": "Cline中文汉化版，Cline是一款在您的 IDE 中运行的自主编程助手，经您许可后可以创建/编辑文件、运行命令、使用浏览器等功能。",
	"version": "3.25.2",
	"icon": "assets/icons/icon.png",
	"engines": {
		"vscode": "^1.84.0"
	},
	"author": {
		"name": "HybridTalentComputing"
	},
	"license": "Apache-2.0",
	"publisher": "HybridTalentComputing",
	"repository": {
		"type": "git",
		"url": "https://github.com/HybridTalentComputing/cline-chinese"
	},
	"homepage": "https://github.com/HybridTalentComputing/cline-chinese",
	"categories": [
		"AI",
		"Chat",
		"Programming Languages",
		"Education",
		"Snippets",
		"Testing"
	],
	"keywords": [
		"cline",
		"claude",
		"dev",
		"mcp",
		"openrouter",
		"coding",
		"agent",
		"autonomous",
		"chatgpt",
		"sonnet",
		"ai",
		"llama"
	],
	"activationEvents": [
		"onLanguage",
		"onStartupFinished",
		"workspaceContains:evals.env"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"walkthroughs": [
			{
				"id": "ClineWalkthrough",
				"title": "认识 Cline，您的新编码伙伴",
				"description": "Cline 像开发者一样编码，因为它像开发者一样思考。这里有 5 种让它工作的方法：",
				"steps": [
					{
						"id": "welcome",
						"title": "从目标开始，而不仅仅是提示",
						"description": "告诉 Cline 您想要实现什么。它计划、询问，然后编码，就像一个真正的伙伴。",
						"media": {
							"markdown": "walkthrough/step1.md"
						}
					},
					{
						"id": "learn",
						"title": "让 Cline 学习您的代码库",
						"description": "将 Cline 指向您的项目。它建立理解以做出智能、上下文感知的更改。",
						"media": {
							"markdown": "walkthrough/step2.md"
						}
					},
					{
						"id": "advanced-features",
						"title": "始终使用最佳 AI 模型",
						"description": "Cline 为您提供最先进的 AI 能力，连接到顶级模型（Anthropic、Gemini、OpenAI 等）。",
						"media": {
							"markdown": "walkthrough/step3.md"
						}
					},
					{
						"id": "mcp",
						"title": "使用强大工具扩展（MCP）",
						"description": "连接到数据库、API，或在 MCP 市场中发现新功能。",
						"media": {
							"markdown": "walkthrough/step4.md"
						}
					},
					{
						"id": "getting-started",
						"title": "您始终掌控一切",
						"description": "审查 Cline 的计划和差异。在更改发生前批准它们。没有意外。",
						"media": {
							"markdown": "walkthrough/step5.md"
						},
						"content": {
							"path": "walkthrough/step5.md"
						}
					}
				]
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "cline-chinese-ActivityBar",
					"title": "Cline Chinese (⌘+')",
					"icon": "assets/icons/icon.svg",
					"when": "isMac"
				},
				{
					"id": "cline-chinese-ActivityBar",
					"title": "Cline Chinese (Ctrl+')",
					"icon": "assets/icons/icon.svg",
					"when": "!isMac"
				}
			]
		},
		"views": {
			"cline-chinese-ActivityBar": [
				{
					"type": "webview",
					"id": "clineChinese.SidebarProvider",
					"name": "",
					"icon": "assets/icons/icon.svg"
				}
			]
		},
		"commands": [
			{
				"command": "clineChinese.plusButtonClicked",
				"title": "新任务",
				"icon": "$(add)"
			},
			{
				"command": "clineChinese.mcpButtonClicked",
				"title": "MCP 服务器",
				"icon": "$(server)"
			},
			{
				"command": "clineChinese.historyButtonClicked",
				"title": "历史记录",
				"icon": "$(history)"
			},
			{
				"command": "clineChinese.popoutButtonClicked",
				"title": "在编辑器中打开",
				"icon": "$(link-external)"
			},
			{
				"command": "clineChinese.accountButtonClicked",
				"title": "账户",
				"icon": "$(account)"
			},
			{
				"command": "clineChinese.settingsButtonClicked",
				"title": "设置",
				"icon": "$(settings-gear)"
			},
			{
				"command": "clineChinese.openInNewTab",
				"title": "在新标签页中打开",
				"category": "clineChinese"
			},
			{
				"command": "clineChinese.dev.createTestTasks",
				"title": "创建测试任务",
				"category": "clineChinese",
				"when": "clineChinese.isDevMode"
			},
			{
				"command": "clineChinese.addToChat",
				"title": "添加到 Cline",
				"category": "clineChinese"
			},
			{
				"command": "clineChinese.addTerminalOutputToChat",
				"title": "添加到 Cline",
				"category": "clineChinese"
			},
			{
				"command": "clineChinese.focusChatInput",
				"title": "跳转到聊天输入",
				"category": "clineChinese"
			},
			{
				"command": "clineChinese.generateGitCommitMessage",
				"title": "使用 Cline 生成提交消息",
				"category": "clineChinese",
				"icon": "$(robot)"
			},
			{
				"command": "clineChinese.abortGitCommitMessage",
				"title": "使用 Cline 生成提交消息 - 停止",
				"category": "clineChinese",
				"icon": "$(debug-stop)"
			},
			{
				"command": "clineChinese.explainCode",
				"title": "使用 Cline 解释",
				"category": "clineChinese"
			},
			{
				"command": "clineChinese.improveCode",
				"title": "使用 Cline 改进",
				"category": "clineChinese"
			},
			{
				"command": "clineChinese.openWalkthrough",
				"title": "打开引导教程",
				"category": "clineChinese"
			}
		],
		"keybindings": [
			{
				"command": "clineChinese.addToChat",
				"key": "cmd+'",
				"mac": "cmd+'",
				"win": "ctrl+'",
				"linux": "ctrl+'",
				"when": "editorHasSelection"
			},
			{
				"command": "clineChinese.generateGitCommitMessage",
				"when": "config.git.enabled && scmProvider == git"
			},
			{
				"command": "clineChinese.focusChatInput",
				"key": "cmd+'",
				"mac": "cmd+'",
				"win": "ctrl+'",
				"linux": "ctrl+'",
				"when": "!editorHasSelection"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "clineChinese.plusButtonClicked",
					"group": "navigation@1",
					"when": "view == clineChinese.SidebarProvider"
				},
				{
					"command": "clineChinese.mcpButtonClicked",
					"group": "navigation@2",
					"when": "view == clineChinese.SidebarProvider"
				},
				{
					"command": "clineChinese.historyButtonClicked",
					"group": "navigation@3",
					"when": "view == clineChinese.SidebarProvider"
				},
				{
					"command": "clineChinese.popoutButtonClicked",
					"group": "navigation@4",
					"when": "view == clineChinese.SidebarProvider"
				},
				{
					"command": "clineChinese.accountButtonClicked",
					"group": "navigation@5",
					"when": "view == clineChinese.SidebarProvider"
				},
				{
					"command": "clineChinese.settingsButtonClicked",
					"group": "navigation@6",
					"when": "view == clineChinese.SidebarProvider"
				}
			],
			"editor/title": [
				{
					"command": "clineChinese.plusButtonClicked",
					"group": "navigation@1",
					"when": "activeWebviewPanelId == clineChinese.TabPanelProvider"
				},
				{
					"command": "clineChinese.mcpButtonClicked",
					"group": "navigation@2",
					"when": "activeWebviewPanelId == clineChinese.TabPanelProvider"
				},
				{
					"command": "clineChinese.historyButtonClicked",
					"group": "navigation@3",
					"when": "activeWebviewPanelId == clineChinese.TabPanelProvider"
				},
				{
					"command": "clineChinese.popoutButtonClicked",
					"group": "navigation@4",
					"when": "activeWebviewPanelId == clineChinese.TabPanelProvider"
				},
				{
					"command": "clineChinese.accountButtonClicked",
					"group": "navigation@5",
					"when": "activeWebviewPanelId == clineChinese.TabPanelProvider"
				},
				{
					"command": "clineChinese.settingsButtonClicked",
					"group": "navigation@6",
					"when": "activeWebviewPanelId == clineChinese.TabPanelProvider"
				}
			],
			"editor/context": [
				{
					"command": "clineChinese.addToChat",
					"group": "navigation",
					"when": "editorHasSelection"
				}
			],
			"terminal/context": [
				{
					"command": "clineChinese.addTerminalOutputToChat",
					"group": "navigation"
				}
			],
			"scm/title": [
				{
					"command": "clineChinese.generateGitCommitMessage",
					"group": "navigation",
					"when": "config.git.enabled && scmProvider == git && !clineChinese.isGeneratingCommit"
				},
				{
					"command": "clineChinese.abortGitCommitMessage",
					"group": "navigation",
					"when": "config.git.enabled && scmProvider == git && clineChinese.isGeneratingCommit"
				}
			],
			"commandPalette": [
				{
					"command": "clineChinese.generateGitCommitMessage",
					"when": "config.git.enabled && scmProvider == git && !clineChinese.isGeneratingCommit"
				},
				{
					"command": "clineChinese.abortGitCommitMessage",
					"when": "config.git.enabled && scmProvider == git && clineChinese.isGeneratingCommit"
				}
			]
		},
		"configuration": {
			"title": "Cline Chinese",
			"properties": {}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "npm run check-types && npm run lint && node esbuild.mjs",
		"compile-standalone": "npm run check-types && npm run lint && node esbuild.mjs --standalone",
		"postcompile-standalone": "node scripts/package-standalone.mjs",
		"watch": "npm-run-all -p watch:*",
		"watch:esbuild": "node esbuild.mjs --watch",
		"watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
		"package": "npm run check-types && npm run build:webview && npm run lint && node esbuild.mjs --production",
		"protos": "node scripts/build-proto.mjs",
		"postprotos": "prettier src/shared/proto src/core/controller src/hosts/ webview-ui/src/services src/generated --write --log-level warn",
		"clean": "rimraf dist dist-standalone webview-ui/build src/generated out/",
		"compile-tests": "node ./scripts/build-tests.js",
		"watch-tests": "tsc -p . -w --outDir out",
		"check-types": "npm run protos && npx tsc --noEmit && cd webview-ui && npx tsc -b --noEmit",
		"lint": "eslint src --ext ts && eslint webview-ui/src --ext ts && buf lint && cd webview-ui && npm run lint",
		"format": "prettier . --check",
		"format:fix": "prettier . --write",
		"pretest": "npm run compile && npm run compile-tests && npm run compile-standalone && npm run lint",
		"test": "npm-run-all test:unit test:integration",
		"test:ci": "node scripts/test-ci.js",
		"test:integration": "vscode-test",
		"test:unit": "TS_NODE_PROJECT='./tsconfig.unit-test.json' mocha",
		"test:coverage": "vscode-test --coverage",
		"e2e": "playwright test -c playwright.config.ts",
		"test:e2e": "playwright install && vsce package --no-dependencies --allow-package-secrets sendgrid --out dist/e2e.vsix && node src/test/e2e/utils/build.mjs && playwright test",
		"test:e2e:optimal": "vsce package --no-dependencies --allow-package-secrets sendgrid --out dist/e2e.vsix && node src/test/e2e/utils/build.mjs && playwright test",
		"install:all": "npm install && cd webview-ui && npm install",
		"dev:webview": "cd webview-ui && npm run dev",
		"build:webview": "cd webview-ui && npm run build",
		"test:webview": "cd webview-ui && npm run test",
		"publish:marketplace": "vsce publish && ovsx publish",
		"publish:marketplace:prerelease": "vsce publish --pre-release && ovsx publish --pre-release",
		"prepare": "husky",
		"changeset": "changeset",
		"version-packages": "changeset version",
		"docs": "cd docs && npm run dev",
		"docs:check-links": "cd docs && npm run check",
		"docs:rename-file": "cd docs && npm run rename",
		"report-issue": "node scripts/report-issue.js"
	},
	"lint-staged": {
		"*": [
			"prettier --write --ignore-unknown --log-level=log"
		]
	},
	"devDependencies": {
